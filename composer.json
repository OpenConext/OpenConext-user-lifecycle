{
    "name": "openconext/user-lifecycle",
    "description": "OpenConext User Lifecycle, a project used to trigger deprovisioning of OpenConext users from the OpenConext suite.",
    "license": "Apache-2.0",
    "type": "project",
    "autoload": {
        "psr-4": {
            "OpenConext\\": "src/OpenConext"
        },
        "classmap": [
            "src/Kernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OpenConext\\": "src\\OpenConext",
            "OpenConext\\UserLifecycle\\Tests\\Integration\\": "tests/integration"
        }
    },
    "require": {
        "php": "^7.2",
        "doctrine/doctrine-bundle": "~2.0",
        "doctrine/doctrine-cache-bundle": "^1.4",
        "doctrine/orm": "^2.10",
        "guzzlehttp/guzzle": "^6.3",
        "incenteev/composer-parameter-handler": "^2.0",
        "openconext/monitor-bundle": "dev-master",
        "sensio/framework-extra-bundle": "^6.0",
        "symfony/cache": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.18",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-kernel": "4.4.*",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/security-bundle": "4.4.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "malukenho/docheader": "^0.1.6",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "mockery/mockery": "^1.1",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpcov": "^6.0",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^2.8",
        "symfony/phpunit-bridge": "^3.0",
        "symfony/var-dumper": "4.4.*",
        "symfony/web-server-bundle": "4.4.*"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/OpenConext/Monitor-bundle.git"
        }
    ],

    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "@auto-scripts"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2.5"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "config/legacy/parameters.yml"
        }
    }
}
